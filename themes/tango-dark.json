{
    "$schema": "vscode://schemas/color-theme",
    "name": "tango",
    "type": "dark",
    "include": "./tokens-dark.json",
    "colors": {
        // Blue
        "tab.activeBorderTop": "#3465a4",
        "activityBarBadge.background": "#3465a4",

        // FG Grays
        "activityBar.foreground": "#babdb6",
        "statusBar.foreground": "#babdb6",
        "sideBar.foreground": "#babdb6",
        "editorLineNumber.foreground": "#555753",
        "editorRuler.foreground": "#2e3436",
        "editor.selectionBackground": "#2e3436",

        // Titlebar
        "titleBar.activeBackground": "#282828",
        "titleBar.inactiveBackground": "#202020",

        // Darkest
        "editorGroupHeader.tabsBackground": "#181818",
        "panel.background": "#181818",
        "tab.inactiveBackground": "#181818",
        "activityBar.background": "#181818",
        "statusBar.background": "#181818",
        "sideBar.background": "#181818",
        "editor.background": "#101010",
        "terminal.background": "#101010",

        // Hide/ignore
        "panel.border": "#00000000",
        "sideBarSectionHeader.background": "#00000000",
    }
}
