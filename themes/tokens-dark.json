{
    "$schema": "vscode://schemas/color-theme",
    "name": "tokens",
	"tokenColors": [
		{
			"scope": [
                "strong",
                "markup.bold",
                "markup.heading",
            ],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
                "emphasis",
                "comment",
                "markup.italic",
            ],
			"settings": {
				"fontStyle": "italic"
			}
		},
        {
			"scope": [
                "markup.underline",
            ],
			"settings": {
				"fontStyle": "underline"
			}
        },
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ef2929"
			}
        },
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#92659a"
			}
		},
        {
			"scope": "header", // ?
			"settings": {
				"foreground": "#204a87"
			}
        },
		{
			"scope": [
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#d3d7cf"
			}
		},
        {
			"scope": [
                "meta.template.expression",
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
                "storage.modifier.package.java",
				"source.groovy.embedded",
			],
			"settings": {
				"foreground": "#babdb6"
			}
        },
		{
			"scope": [
                "punctuation.definition.tag", // Tag brackets
                "entity.name.label",
            ],
			"settings": {
				"foreground": "#888a85"
			}
		},
		{
			"scope": [
                "comment",
                "comment.block.documentation.js punctuation.definition.block.tag.jsdoc",
                "comment.block.documentation.js entity.name.type.instance.jsdoc",
                "comment.block.documentation.js storage.type.class.jsdoc",
                "comment.block.documentation.js variable.other.jsdoc",
                "comment.block.documentation.phpdoc.php punctuation",
                "comment.block.documentation.phpdoc.php support",
                "comment.block.documentation.phpdoc.php keyword",
                "comment.block.documentation.phpdoc.php keyword.other",
            ],
			"settings": {
				"foreground": "#555753"
			}
		},
		{
			"scope": [
                "constant.character",
                "markup.changed",
                "markup.heading",
                "markup.bold",
				"meta.preprocessor",
                "meta.diff.header",
                "entity.name.function.preprocessor",
                "punctuation.section.embedded",
                "punctuation.section.embedded.end.php",
                "punctuation.section.embedded.begin.php",
                "punctuation.definition.template-expression.end",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.list.begin.markdown",
				"entity.name.variable",
                "entity.name.tag.css",
                "entity.other.attribute-name.scss",
                "entity.other.attribute-name.ccss",
				// "entity.other.attribute-name.attribute.scss",
                // "entity.other.attribute-name.class.css",
				// "entity.other.attribute-name.class.mixin.css",
				// "entity.other.attribute-name.id.css",
				// "entity.other.attribute-name.parent-selector.css",
				// "entity.other.attribute-name.pseudo-class.css",
				// "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "meta.definition.variable.name",
                "meta.object-literal.key",
            ],
			"settings": {
				// "foreground": "#729fcf"
				"foreground": "#c4a000"
			}
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag", // Tag names
            ],
            "settings": {
				"foreground": "#edd400"
			}
        },
        {
            "name": "Keys, definitions",
            "scope": [
                "entity.other.attribute-name",
            ],
            "settings": {
				"foreground": "#c4a000"
			}
        },
		{
            "name": "Variables",
			"scope": [
                "variable",
				"support.variable",
                "support.function.git-rebase",
                "support.type.property-name",
				"support.type.vendored.property-name",
                "meta.structure.dictionary.key.python",
			],
			"settings": {
				"foreground": "#07c7ca"
			}
        },
		{
			"name": "Constants and enums",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#63e9e9",
			}
        },
		{
			"name": "This, self",
			"scope": [
                "variable.language",
                "constant.language",
			],
			"settings": {
				"foreground": "#cc0000",
			}
        },
		{
			"scope": [
				"constant.numeric",
                "constant.sha.git-rebase",
				"variable.other.enummember",
                "markup.inserted",
                "meta.preprocessor.numeric",
				"entity.name.operator.custom-literal.number",
			],
			"settings": {
				"foreground": "#8ae234"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
                "entity.name.operator.custom-literal",
                "constant.character.escape",
			],
			"settings": {
				// "foreground": "#e9b96e"
				"foreground": "#729fcf"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
                "entity.name.class",
                "meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class",
                "punctuation.definition.quote.begin.markdown",
            ],
			"settings": {
				"foreground": "#63e9e9"
			}
        },
		{
			"scope": [
                "meta.embedded",
            ],
			"settings": {
				"foreground": "#07c7ca"
			}
        },
		{
			"name": "Control flow",
			"scope": [
                "keyword",
                "storage",
                "storage.type",
				"storage.modifier",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#92659a"
			}
        },
		{
			"name": "Special keywords",
			"scope": [
                // "keyword.operator",
                // "keyword.other",
                // "keyword.control",
			],
			"settings": {
				"foreground": "#c19fbe"
			}
		},
		{
			"name": "Strings, CSS property value, Regular expression groups ++",
			"scope": [
                "string",
                "string.tag",
                "string.value",
                "markup.deleted",
                "markup.inline.raw",
                "meta.preprocessor.string",
                "entity.name.operator.custom-literal.string",
                "support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
                "support.other.parenthesis.regexp",
			],
			"settings": {
				"foreground": "#4e9a06"
			}
		}
	]
}
